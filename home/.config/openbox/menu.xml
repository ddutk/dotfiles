<menu id="openbox-config-menu" label="Openbox Config">
  <item label="Edit menu.xml" icon="/usr/share/icons/elementary/actions/48/bookmark-new.svg">
    <action name="Execute">
      <command>subl3 ~/.config/openbox/menu.xml</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <item label="Edit rc.xml" icon="/usr/share/icons/elementary/mimes/48/text-x-makefile.svg">
    <action name="Execute">
      <command>subl3 ~/.config/openbox/rc.xml</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <item label="OBConf" icon="/usr/share/icons/elementary/categories/48/preferences-desktop.svg">
    <action name="Execute">
      <command>obconf</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <item label="Reconfigure Openbox" icon="/usr/share/icons/elementary/actions/48/view-refresh.svg">
    <action name="Reconfigure" />
  </item>
</menu>

<menu id="xdg-pipe-menu" label="XDG Menu" execute="xdg_menu --format openbox3-pipe --root-menu /etc/xdg/menus/arch-applications.menu" />

