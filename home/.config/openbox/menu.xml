<menu id="end-session-menu" label="End Session">
  <item label="Log Out" icon="/usr/share/icons/elementary/actions/48/system-log-out.svg" >
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
  <item label="Reboot" icon="/usr/share/icons/elementary/actions/48/view-refresh.svg">
    <action name="Execute">
      <command>gksu 'systemctl reboot'</command>
      <startupnotify><enabled>no</enabled></startupnotify>
    </action>
  </item>
  <item label="Shut Down" icon="/usr/share/icons/elementary/actions/48/system-shutdown.svg">
    <action name="Execute">
      <command>gksu 'systemctl poweroff'</command>
      <startupnotify><enabled>no</enabled></startupnotify>
    </action>
  </item>
</menu>

