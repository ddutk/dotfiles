#!/bin/sh
# Back up Evernote database

# This will be different on other operating systems
EVERNOTE_DIR="${HOME}/Library/Application Support/com.evernote.Evernote/accounts"
OUTDIR="$HOME/Dropbox/Storage/Charles/Backup/"

tar cvf evernote.tar "${EVERNOTE_DIR}"
xz -9e evernote.tar
mv ./evernote.tar.xz ${OUTDIR}

