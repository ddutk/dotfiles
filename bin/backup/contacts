#!/bin/sh
# Back up Google Contacts

OUTDIR="${HOME}/Dropbox/Charles/Storage/Backup"

echo "DOWNLOAD the google.csv file from Google Contacts and place it in the current directory."
read -p "Press any key to continue..."

mv google.csv contacts.csv
lzma -9 contacts.csv
mv contacts.csv.lzma $OUTDIR

exit 0

~/bin/backup/contacts-gmail.py

while IFS='
' read -r line
do
    case $line in
        "ORG:"*)
            echo $line | sed "s/;//g"; 
            ;;
        *)
            echo $line
            ;;
    esac
done < contacts.vcf >> test.vcf

mv test.vcf contacts.vcf
lzma -9 contacts.vcf

mv contacts.vcf.lzma ${OUTDIR}
